<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hf.project.common.mapper.common.UserMapper">

    <select id="query" parameterType="java.lang.String" resultMap="UserMap">
        SELECT
        PNL,
        ChName
        FROM
        rt_UsersList
        WHERE PNL = #{workCode}
    </select>

    <resultMap id="UserMap" type="User">
        <id column="PNL" jdbcType="VARCHAR" property="userCode"/>
        <result column="ChName" jdbcType="VARCHAR" property="userName"/>
    </resultMap>

</mapper>
